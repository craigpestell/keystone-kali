{{#switch postLayout.key}}
	{{#case "slide-one"}}
		{{> post-layouts/blog/slide-one}}
	{{/case}}
	{{#case "youtube-full"}}
		{{> post-layouts/blog/youtube-full}}
	{{/case}}
	{{#case "technology"}}
		{{> post-layouts/blog/technology}}
	{{/case}}
	{{#case "post-full-width"}}
		POST-FULL-WIDTH
		{{> post-layouts/blog/post-full-width}}
	{{/case}}
	{{#case "post-basic"}}
