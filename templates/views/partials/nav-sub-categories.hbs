<!-- subnav here -->
<div class="container-fluid">
<nav id="nav-product-categories" class="navbar navbar-static-top hidden-xs subnav" role="navigation">
	{{#container}}
		<ul class="navbar navbar-nav">
		{{#each this}}
			{{#if name}}
			<li class="{{#ifeq key ../../subCategory}}active{{/ifeq}}">
				<a class="nav-anchor text-uppercase text-nowrap" href="/{{parentCategory.slug}}#{{slug}}">{{name}}</a>
			</li>
			{{/if}}
		{{/each}}
		</ul>
	{{/container}}
</nav>
</div>
