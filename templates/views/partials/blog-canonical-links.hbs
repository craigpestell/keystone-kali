{{#ifCond @root.section "==" "republik"}}
	{{#if post}}
		<link rel="canonical" href="https://bike.kaliprotectives.com/republik/post/{{post.key}}"/>
	{{else}}
  
  	<link rel="canonical" href="https://bike.kaliprotectives.com/republik{{#if data.categoryDoc}}/{{data.categoryDoc.key}}{{/if}}{{#ifCond data.posts.currentPage ">" 1}}?page={{data.posts.currentPage}}{{/ifCond}}" />
    {{#if data.posts.previous}}
      <link rel="prev" href="https://bike.kaliprotectives.com/republik/?page={{data.posts.previous}}" />
    {{/if}}
    {{#if data.posts.next}}
      <link rel="next" href=" https://bike.kaliprotectives.com/republik/?page={{data.posts.next}}" />
    {{/if}}
	{{/if}}
{{/ifCond}}
{{#ifCond @root.section "==" "technology"}}
	{{#ifCond post.key "!=" "technology"}}
		<link rel="canonical" href="https://kaliprotectives.com/technology/post/{{post.key}}"/>
	{{else}}
		<link rel="canonical" href="https://kaliprotectives.com/technology"/>
	{{/ifCond}}
{{/ifCond}}
