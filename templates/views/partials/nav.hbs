<nav id="nav-main" class="navbar navbar-static-top" role="navigation">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar-collapse-main">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="//{{domainAndPort}}">
				<div class="navbar-logo" style="background: url({{cloudinaryUrl siteSettings.nav-logo-large.imageValue format="jpg" width=150 }}); background: url({{cloudinaryUrl siteSettings.nav-logo-large.imageValue width=150 }});background-size: 150px 50px;"></div>
            </a>
        </div>
        <div class="collapse navbar-collapse" id="navbar-collapse-main">
        <ul class="nav navbar-nav">
			{{# each navLinks}}
				{{#unless @last}}
					{{#if dropdown}}
						<li class="{{#ifeq key ../../../page}} active{{/ifeq}}"><a href="{{href}}">{{label}}</a></li>
					{{else}}
			            <li class="{{#ifeq key ../../../page}} active{{/ifeq}}"><a href="{{href}}">{{label}}</a></li>
					{{/if}}
				{{else}}
		</ul>
		
		<ul class="nav navbar-nav navbar-right">
			<li>
				<div id="nav-discipline">
					<ul>
						{{#ifCond ../../params.discipline.slug "==" "bike"}}
							<li class="active">Bike</li>
						{{else}}
							<li><a href="//bike.{{../../../domainAndPort}}">Bike</a></li>
						{{/ifCond}}
						{{#ifCond ../../params.discipline.slug "==" "moto"}}
							<li class="active">Moto</li>
						{{else}}
							<li><a href="//moto.{{../../../domainAndPort}}">Moto</a></li>
						{{/ifCond}}
					</ul>
				</div>
			</li>
            <li class="{{#ifeq key ../../page}} active{{/ifeq}}"><a href="{{href}}">{{label}}</a></li>
		</ul>
				{{/unless}}
			{{/each}}
        </div>
    </div>
</nav>
